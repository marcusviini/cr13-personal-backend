        "grpc": "^1.24.5",
        "helmet": "^4.4.1",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.2.0",
        "nodemon": "^2.0.7",
        "npm-run-all": "^4.1.5",
        "pg": "^8.5.1",
        "pg-hstore": "^2.3.3",
        "pm2": "^4.5.5",
        "sequelize": "^6.4.0",
        "sequelize-cli": "^6.2.0",
